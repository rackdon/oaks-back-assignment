{
  "name": "oaks-back-assignment",
  "version": "1.0.0",
  "main": "app.js",
  "scripts": {
    "local": "nodemon dotenv_config_path=./environments/.env.local",
    "local:memory": "nodemon dotenv_config_path=./environments/.env.memory.local",
    "build": "rimraf ./build && tsc",
    "start": "npm run build && node build/app.js",
    "lint": "eslint . --ext .ts",
    "test": "jest --runInBand",
    "integration-test": "jest --config=./jest.integration.config.ts --runInBand",
    "all-test": "npm run test && npm run integration-test",
    "test:dev": "jest --watchAll",
    "integration-test:dev": "jest --config=./jest.integration.config.ts --watchAll",
    "test:coverage": "jest --coverage --runInBand && jest --config=./jest.integration.config.ts --coverage  --runInBand && cat coverageIntegration/lcov.info >> coverage/lcov.info",
    "prettier-format": "run-script-os",
    "prettier-format:win32": "prettier --config .prettierrc \"./src/**/*.ts\" --write",
    "prettier-format:darwin:linux": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prettier-format:default": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "prepare": "husky install",
    "migrate": "node-pg-migrate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rackdon/oaks-back-assignment.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rackdon/oaks-back-assignment/issues"
  },
  "homepage": "https://github.com/rackdon/oaks-back-assignment#readme",
  "description": "",
  "dependencies": {
    "@godaddy/terminus": "^4.10.2",
    "@sentry/node": "^6.19.7",
    "@sentry/tracing": "^6.19.7",
    "axios": "^0.27.2",
    "class-validator": "^0.13.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "envalid": "^7.3.1",
    "express": "^4.18.1",
    "express-graphql": "^0.12.0",
    "graphql": "^16.5.0",
    "graphql-scalars": "^1.17.0",
    "graphql-sequelize": "^9.4.3",
    "helmet": "^5.0.2",
    "morgan": "^1.10.0",
    "node-pg-migrate": "^6.2.1",
    "pg": "^8.7.3",
    "run-script-os": "^1.1.6",
    "sequelize": "^6.19.2",
    "winston": "^3.7.2",
    "winston-sentry-log": "^1.0.26"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.3.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "nock": "^13.2.4",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.4",
    "uuid": "^8.3.2"
  }
}
